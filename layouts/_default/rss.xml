{{- printf "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"yes\"?>" | safeHTML }}
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ .Site.Title }}</title>
    <link>{{ .Permalink }}</link>
    <description>These are some good ass frames</description>
    <generator>Hugo -- gohugo.io</generator>
    {{ with .Site.LanguageCode -}}
    <language>{{ . }}</language>
    {{ end -}}
    {{ with .Site.Copyright -}}
    <copyright>{{ . }}</copyright>
    {{ end -}}
    {{ with .Site.Lastmod -}}
    <lastBuildDate>{{ .Format "Mon, 02 Jan 2006 15:04:05 -0700" | safeHTML }}</lastBuildDate>
    {{ end -}}
    {{- with .OutputFormats.Get "RSS" -}}
    {{ printf "<atom:link href=%q rel=\"self\" type=%q />" .Permalink .MediaType | safeHTML }}
    {{- end -}}
    {{ range first 20 .Site.RegularPages -}}
    {{ $img := .Resources.GetMatch "frame.*" -}}
    {{ $alt := .RawContent | plainify | strings.TrimSpace -}}
    <item>
      <title>{{ .Title }}</title>
      <link>{{ .Permalink }}</link>
      <pubDate>{{ .Date.Format "Mon, 02 Jan 2006 15:04:05 -0700" | safeHTML }}</pubDate>
      <guid>{{ .Permalink }}</guid>
      <description>{{ with $img }}&lt;img src=&#34;{{ .Permalink }}&#34; alt=&#34;{{ $alt }}&#34;&gt;{{ end }}</description>
      {{- with $img }}
      <enclosure url="{{ .Permalink }}" type="image/jpeg" length="0" />
      {{- end }}
    </item>
    {{ end -}}
  </channel>
</rss>
